<div class="row">
  <div class="col-md-2 emotion-select">
    <div class="row">
      <div class="col-md-1">
        <br><br>
      </div>
      <br>
      <div class="col-md-9 emotions">
        <div class="happy">
          Happy
          <input class="happy-slide" type="range" name="name" min="0" max="10" value="0"><br>
        </div>
        <div class="sad">
          Sad
          <input class="sad-slide" type="range" name="name" min="0" max="10" value="0"><br>
        </div>
        <input type="range" name="name" min="0" max="10" value="0"><br>
        <input type="range" name="name" min="0" max="10" value="0"><br>
        <input type="range" name="name" min="0" max="10" value="0"><br>
        <input type="range" name="name" min="0" max="10" value="0"><br>
        <div class="">
          <br><br>
          <br><br>
          <br><br>
          <br><br>
          <br><br>
          <br><br>
          <br><br>
          <br><br><br><br><br><br><br><br>
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-10">
    <br>
    <%= form_for @entry do |f| %>
    <div class="form-group">
      <%= f.text_field :tag, id: "tag", value: "Tag your post with a descriptive title" %>
    </div>

    <div class="form-group">
      <%= f.text_area :body, value: "New Entry" %>
    </div>

    <div class="create-entry">
      <%= f.submit class:"submit-entry"%>
    </div>

    <% end %>
  </div>

</div>
